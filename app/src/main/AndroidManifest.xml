<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team20.pitscouting">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScoutActivity"></activity>
        <!--android:theme="@style/AppTheme.CustomTheme"-->
        <activity android:name=".OtherPop" android:theme="@style/AppTheme.CustomTheme"></activity>
        <activity android:name=".CommentsPop" android:theme="@style/AppTheme.CustomTheme"></activity>
        <activity android:name=".ExitPrompt" android:theme="@style/AppTheme.CustomTheme"></activity>
    </application>
</manifest>